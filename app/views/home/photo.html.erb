Hello this is photo page
Max page: <%= @pages %>
<div class="container">
    <div class="grid">
        <div class="grid-sizer"></div>
        <!--<div class="grid-gutter-sizer"></div>-->
      <% @photos.each do |photo| %>
         <div class="grid-item">
            <%= image_tag photo, alt: "random" %>
          </div>
      <% end %>
    </div>
</div>

<% if params[:page].to_i <= 2 %>
    <nav id="page-nav" style="display: none;">
      <a href="/photos/page/2"></a>
    </nav>
<% end %>