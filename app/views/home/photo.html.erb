Hello this is photo page
Max page: <%= @pages %>
<div class="container">
    <div class="grid">
        <div class="grid-sizer"></div>
        <!--<div class="grid-gutter-sizer"></div>-->
      <% @photos.each do |photo| %>
         <div class="grid-item">
            <%= image_tag photo, alt: "random" %>
          </div>
      <% end %>
    </div>
</div>

<% if @next_page %>
    <nav id="page-nav" style="display: none;">
      <a href="/photos/page/<%= @next_page %>"></a>
    </nav>
<% end %>
